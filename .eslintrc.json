{
  "env": {
    "commonjs": true,
    "es2020": true,
    "node": true
  },
  "extends": ["google", "prettier", "plugin:node/recommended"],
  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "prettier/prettier": "error",
    "no-unused-vars": "warn",
    "no-console": "warn",
    "no-mixed-spaces-and-tabs": "warn"
  }
}
